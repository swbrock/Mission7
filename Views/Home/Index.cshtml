@{
    ViewData["Title"] = "Home Page";
}

@model BooksViewModel

<h1>The Book Store</h1>

<table class="table table-bordered table-striped bg-success">
    <tr>
        <th>
            Title
        </th>
        <th>
            Author
        </th>
        <th>
            Publisher
        </th>
        <th>
            ISBN
        </th>
        <th>
            Classification
        </th>
        <th>
            Category
        </th>
        <th>
            Number Of Pages
        </th>
        <th>
            Price
        </th>
    </tr>
        @foreach (Books b in Model.Books)
        {
            <partial name="ProjectSummary" model="b"/>

    }
    </table>


<div page-blah="@Model.BookInfo" page-action="Index"></div>