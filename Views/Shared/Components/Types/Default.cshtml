@{
}

@model IEnumerable<string>
<div class="d-grid">
    <a asp-controller="Home" asp-action="Index" asp-route-Category="" asp-route-pageNum ="1" class="btn btn-outline-secondary">Home</a>

    @foreach (string t in Model)
            {
                <a asp-controller="Home" asp-action="Index" asp-route-projectType="@t" asp-route-pageNum="1" class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")">@t</a>
            }
</div>